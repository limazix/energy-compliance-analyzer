{
  "type": "module",
  "name": "electric-magnitudes-analizer",
  "version": "0.1.0",
  "private": true,
  "license": "Apache-2.0",
  "scripts": {
    "dev": "next dev --turbopack -p 9002",
    "genkit:dev": "genkit start -- tsx src/ai/dev.ts",
    "genkit:watch": "genkit start -- tsx --watch src/ai/dev.ts",
    "build": "next build",
    "start": "next start",
    "lint": "cross-env NODE_OPTIONS=--max-old-space-size=4096 eslint . --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md,css,yml}\" --ignore-path .prettierignore",
    "typecheck": "tsc --noEmit",
    "docs:generate": "typedoc",
    "emulators:start": "firebase emulators:start --import=./firebase-emulator-data --export-on-exit",
    "emulators:dev": "mkdir -p ./firebase-emulator-data && firebase emulators:exec --import=./firebase-emulator-data 'npm run dev'",
    "test": "jest",
    "test:unit:frontend": "cross-env NODE_OPTIONS=--max-old-space-size=4096 jest --selectProjects frontend",
    "test:unit:backend": "cross-env NODE_OPTIONS=--max-old-space-size=4096 jest --selectProjects backend",
    "test:integration": "mkdir -p ./firebase-emulator-data && firebase emulators:exec --import=./firebase-emulator-data --export-on-exit -- jest tests/integration",
    "test:watch": "jest --watch",
    "test:watch:frontend": "jest --selectProjects frontend --watch",
    "test:watch:backend": "jest --selectProjects backend --watch",
    "test:watch:integration": "mkdir -p ./firebase-emulator-data && firebase emulators:exec --import=./firebase-emulator-data --export-on-exit -- jest tests/integration --watch",
    "prepare": "husky",
    "analyze": "cross-env ANALYZE=true next build"
  },
  "dependencies": {
    "@genkit-ai/googleai": "^1.8.0",
    "@genkit-ai/next": "^1.8.0",
    "@hookform/resolvers": "^4.1.3",
    "@next/bundle-analyzer": "^14.2.5",
    "@radix-ui/react-accordion": "^1.2.3",
    "@radix-ui/react-alert-dialog": "^1.1.6",
    "@radix-ui/react-avatar": "^1.1.3",
    "@radix-ui/react-checkbox": "^1.1.4",
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-dropdown-menu": "^2.1.6",
    "@radix-ui/react-label": "^2.1.2",
    "@radix-ui/react-menubar": "^1.1.6",
    "@radix-ui/react-popover": "^1.1.6",
    "@radix-ui/react-progress": "^1.1.2",
    "@radix-ui/react-radio-group": "^1.2.3",
    "@radix-ui/react-scroll-area": "^1.2.3",
    "@radix-ui/react-select": "^2.1.6",
    "@radix-ui/react-separator": "^1.1.2",
    "@radix-ui/react-slider": "^1.2.3",
    "@radix-ui/react-toast": "^1.2.14",
    "@tanstack/react-query": "^5.80.10",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cross-env": "^7.0.3",
    "date-fns": "^3.6.0",
    "dotenv": "^16.5.0",
    "eslint-import-resolver-typescript": "^4.4.3",
    "firebase": "^11.8.1",
    "firebase-admin": "^12.0.0",
    "genkit": "^1.8.0",
    "jest-config": "^30.0.0",
    "jiti": "^2.4.2",
    "lucide-react": "^0.475.0",
    "mermaid": "^10.9.0",
    "next": "15.2.3",
    "next-mdx-remote": "^5.0.0",
    "patch-package": "^8.0.0",
    "react": "^18.3.1",
    "react-day-picker": "^8.10.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.54.2",
    "recharts": "^2.15.1",
    "remark-gfm": "^4.0.0",
    "remark-mermaidjs": "^6.0.0",
    "tailwind-merge": "^3.0.1",
    "tailwindcss-animate": "^1.0.7",
    "ts-node": "^10.9.2",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.4.8",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.12",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^8.34.1",
    "@typescript-eslint/parser": "^8.34.1",
    "eslint": "^9.29.0",
    "eslint-config-next": "15.2.3",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^5.1.2",
    "firebase-tools": "^13.15.0",
    "genkit-cli": "^1.8.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^16.1.2",
    "node-notifier": "^10.0.1",
    "postcss": "^8",
    "prettier": "^3.3.3",
    "tailwindcss": "^3.4.1",
    "ts-jest": "^29.2.3",
    "typedoc": "^0.26.5",
    "typescript": "5.5.4"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "cross-env NODE_OPTIONS=--max-old-space-size=4096 eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,css,yml}": [
      "prettier --write"
    ]
  }
}
