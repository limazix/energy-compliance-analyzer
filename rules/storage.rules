rules_version = '2';
service firebase.storage {
  match /b/{bucket}/o {
    match /user_uploads/{userId}/{analysisId}/{allPaths=**} {
      allow read, write: if request.auth != null && request.auth.uid == userId;
    }
    match /user_reports/{userId}/{analysisId}/{allPaths=**} { 
      allow read, write: if request.auth != null && request.auth.uid == userId;
    }
  }
}
